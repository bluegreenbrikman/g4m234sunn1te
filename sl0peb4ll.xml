<!DOCTYPE html>
<html lang="en-us">
<head>
  <meta charset="utf-8">
  <title>Slope Ball</title>
  <link rel="shortcut icon" href="https://cdn.jsdelivr.net/gh/elite-gamez/slopeee@main/favicon.ico">
  <link rel="stylesheet" href="https://cdn.jsdelivr.net/gh/elite-gamez/slopeee@main/style.css">

  <script src="https://cdn.jsdelivr.net/gh/elite-gamez/slopeee@main/UnityProgress.js"></script>
  <script src="https://cdn.jsdelivr.net/gh/elite-gamez/slopeee@main/UnityLoader.js"></script>

  <script>
    var gameInstance = UnityLoader.instantiate(
      "gameContainer",
      "https://cdn.jsdelivr.net/gh/elite-gamez/slopeee@main/build.json",
      { onProgress: UnityProgress }
    );
  </script>
</head>
<body>
  <div class="webgl-content">
    <div id="gameContainer" style="width: 900px; height: 675px;"></div>
    <div class="footer">
      <div class="webgl-logo"></div>
      <div class="fullscreen" onclick="gameInstance.SetFullscreen(1)"></div>
      <div class="title">Slope Ball</div>
    </div>
  </div>
</body>
</html>
